# Azure Service Principal Credentials (Required for OneLake upload)
AZURE_CLIENT_ID=your_azure_client_id
AZURE_TENANT_ID=your_azure_tenant_id
AZURE_CLIENT_SECRET=your_azure_client_secret

# Microsoft Fabric SQL Database Connection (Required for Drizzle ORM)
MSSQL_SERVER=your_fabric_sql_server.database.fabric.microsoft.com
MSSQL_DATABASE=your_fabric_sql_database_id
MSSQL_PORT=1433

# Microsoft Fabric OneLake Configuration (Required for OneLake upload)
FABRIC_LAKEHOUSE_NAME=your_lakehouse_name.Lakehouse
FABRIC_WORKSPACE_NAME=your_workspace_name
ONELAKE_UPLOAD_FOLDER=Files/uploads
